<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Knightlight_v5_GUI" xmlns:Knightlight_v5_GUI="clr-namespace:Knightlight_v5_GUI.Knightlight_v5_GUI" x:Class="ctrlDimmerChannel"
             d:DataContext="{d:DesignInstance Type={x:Type local:cSettings}}"
             mc:Ignorable="d" 
             d:DesignHeight="266" d:DesignWidth="46"
             Background="Gray">
    <Grid>

        <Knightlight_v5_GUI:VerticalSlider HorizontalAlignment="Center" VerticalAlignment="Top" Height="176" Width="46" Maximum="255" Minimum="0" SmallIncrement="1" LargeIncrement="10" Title="DMX" Unit="a" Margin="0,26,0,0" TickSpacing="50"/>
        
        <Label x:Name="lblTop" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Top" Width="46" Height="26"/>
        <Label x:Name="lblBottom" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Top" Width="46" Height="26" Margin="0,190,0,0"/>
        <Button x:Name="cmdSelect" Content="S" HorizontalAlignment="Center" Margin="0,221,0,0" VerticalAlignment="Top" Width="23" Height="23"/>

    </Grid>
</UserControl>
